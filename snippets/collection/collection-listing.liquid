{% comment %}DO NOT REMOVE THIS FILE! It is used for an alternate layout in the event that the URL '/collections/' is viewed! - Adam Wooding{% endcomment %}
<div class="container">
	<header>
		<div class="page-header">
			<h1>{{ page_title }}</h1>
		</div>
	</header>
	<ul>
	{% for collection in collections %}
	{% unless collection.handle == 'frontpage' %}
	<li><a href="{{ collection.url }}">{{ collection.title }}</a>
	{% endunless %}
	{% endfor %}
	</ul>
</div>